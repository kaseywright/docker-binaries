#!/usr/bin/env sh
set -e

# FORCED_PORTS are osx required only
FORCED_PORTS=${DB_EXPOSE_PORTS:=""}

VERSION=${DB_GO_VERSION:=""}
DEFAULT_DISTRIBUTION=${DB_DISTRIBUTION:=alpine}

export IMAGE_NAME=docker.io/golang
export IMAGE_TAG_VERSION=${DEFAULT_DISTRIBUTION}${VERSION}
export COMMAND_TO_EXECUTE=go

.docker_binary_execute "$@"
